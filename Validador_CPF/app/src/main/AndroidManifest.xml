<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.validador_cpf">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icone_cpf"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icone_cpf"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ValidadorActivity" />
        <activity android:name=".ListaCpf" />
        <activity android:name=".Sobre" />
        <activity android:name=".CuriosidadeCpf"></activity>
    </application>

</manifest>